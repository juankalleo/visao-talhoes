{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_STAC_API_URL": "@vite_stac_api_url",
    "VITE_COPERNICUS_BASE_URL": "@vite_copernicus_base_url"
  },
  "functions": {
    "api/**": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}